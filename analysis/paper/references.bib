@UNPUBLISHED{Garza2022-ro,
  title    = "Single-cell transcriptomics of resected human traumatic brain
              injury tissues reveals acute activation of endogenous
              retroviruses in oligodendrocytes",
  author   = "Garza, Raquel and Sharma, Yogita and Atacho, Diahann and Hamdeh,
